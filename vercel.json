{
  "version": 2,
  "functions": {
    "api/**/*.go": {
      "runtime": "@vercel/go@3.1.0"
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/web/chess.html"
    },
    {
      "source": "/chess.js",
      "destination": "/web/chess.js"
    },
    {
      "source": "/style.css",
      "destination": "/web/style.css"
    },
    {
      "source": "/js/:path*",
      "destination": "/web/js/:path*"
    },
    {
      "source": "/pieces/:path*",
      "destination": "/web/pieces/:path*"
    }
  ]
}
