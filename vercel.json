{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/api/start",
      "dest": "/api/start"
    },
    {
      "src": "/api/moves",
      "dest": "/api/moves"
    },
    {
      "src": "/api/move",
      "dest": "/api/move"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/web/chess.html"
    },
    {
      "source": "/(.*)",
      "destination": "/web/$1"
    }
  ]
}
